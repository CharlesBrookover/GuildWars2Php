{
  "name":              "landfillsoft/guildwars2api",
  "description":       "API for the UI that correlates the data from the Guild Wars 2 API",
  "minimum-stability": "dev",
  "license":           "MIT",
  "authors":           [
    {
      "name":  "Charles Brookover",
      "email": "charlesbrookover@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform-check": true,
    "platform": {
      "php": "8.2"
    }
  },
  "require": {
    "ext-json":           "*",
    "guzzlehttp/guzzle":  "^7.5.0",
    "php":                "^8.2",
    "php-di/php-di":      "^6.4.0",
    "php-di/slim-bridge": "^3.3.0",
    "slim/psr7":          "^1.6",
    "slim/slim":          "^4.11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
