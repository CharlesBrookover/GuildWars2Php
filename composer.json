{
  "name":              "landfillsoft/guildwars2api",
  "description":       "API for the UI that correlates the data from the Guild Wars 2 API",
  "minimum-stability": "dev",
  "license":           "MIT",
  "authors":           [
    {
      "name":  "Charles Brookover",
      "email": "charlesbrookover@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform-check": true
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "slim/slim": "^4.10",
    "php-di/php-di": "^6.4",
    "slim/psr7": "^1.6",
    "guzzlehttp/guzzle": "^7.5",
    "monolog/monolog": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}